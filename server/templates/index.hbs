<script type="text/template" id="post">
  <div class="post">
    <div class="post-header">
      <div class="author-name">
        Dylan Wright
      </div>
      <div class="time-ago">
      <%- date %>
      </div>
    </div>
    <h3>
      <a href="/post/1/<%- url %>/"><%- title %></a>
    </h3>
    <div class="post-image">
      <a href="/post/1/<%- url %>/">
        <img src="<%- img %>" />
      </a>
    </div>
    <div class="post-summary">
      <a href="/post/1/<%- url %>/"><%- lead %></a>
    </div>
    <div class="post-footer">
      <a href="/post/1/<%- url %>">
        <span class="continue-reading">
          continue reading
        </span>
      </a>
    </div>
  </div>
</script>

<script type="text/template" id="full-post">
    <div id="full-post-header">
      <div id="full-post-author">
