      </div>
      <div id="full-post-date">
        <%- date %>
      </div>
    </div>
    <h2>
      <%- title %>
    </h2>
    <div id="full-post-image">
      <img src="<%- img %>" width="100%" />
    </div>
    <div id="full-post-content">
      
    </div>
    <div id="full-post-footer">
    </div>
</script>

